
def tagParser {
    var ret = new Object

    ret def <tag1< @expr >/tag1> {|x|
        print "Tag1"
        return self
    }

    ret def <tag2< @expr >/tag2> {|x|
        print "Tag2"
        return self
    }

    ret def > {print "`>"; self}

    ret def @token {|x| print x; self}

    printDeep ret
    ret prototype = void
    return ret
}

